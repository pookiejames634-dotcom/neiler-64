(footprint "Neiler_GPU_TQFP100" (version 20211014) (generator pcbnew)
  (layer "F.Cu")
  (descr "Neiler GPU, 100-pin TQFP package, 20x20mm body, 0.5mm pitch")
  (tags "TQFP GPU NeilerGPU")
  (attr smd)
  (fp_text reference "REF**" (at 0 -12) (layer "F.SilkS")
    (effects (font (size 1 1) (thickness 0.15)))
  )
  (fp_text value "Neiler_GPU" (at 0 12) (layer "F.Fab")
    (effects (font (size 1 1) (thickness 0.15)))
  )

  ;; Package outline
  (fp_line (start -10 -10) (end 10 -10) (layer "F.SilkS") (width 0.12))
  (fp_line (start 10 -10) (end 10 10) (layer "F.SilkS") (width 0.12))
  (fp_line (start 10 10) (end -10 10) (layer "F.SilkS") (width 0.12))
  (fp_line (start -10 10) (end -10 -10) (layer "F.SilkS") (width 0.12))

  ;; Pin 1 indicator
  (fp_circle (center -8.5 -8.5) (end -7.5 -8.5) (layer "F.SilkS") (width 0.12))

  ;; Courtyard
  (fp_rect (start -11.5 -11.5) (end 11.5 11.5) (layer "F.CrtYd") (width 0.05))

  ;; TQFP100 pads (0.5mm pitch, 25 pins per side)
  ;; Top side (pins 1-25)
  (pad "1" smd rect (at -10.75 -6) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "2" smd rect (at -10.75 -5.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "3" smd rect (at -10.75 -5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "4" smd rect (at -10.75 -4.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "5" smd rect (at -10.75 -4) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "6" smd rect (at -10.75 -3.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "7" smd rect (at -10.75 -3) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "8" smd rect (at -10.75 -2.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "9" smd rect (at -10.75 -2) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "10" smd rect (at -10.75 -1.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "11" smd rect (at -10.75 -1) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "12" smd rect (at -10.75 -0.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "13" smd rect (at -10.75 0) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "14" smd rect (at -10.75 0.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "15" smd rect (at -10.75 1) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "16" smd rect (at -10.75 1.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "17" smd rect (at -10.75 2) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "18" smd rect (at -10.75 2.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "19" smd rect (at -10.75 3) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "20" smd rect (at -10.75 3.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "21" smd rect (at -10.75 4) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
