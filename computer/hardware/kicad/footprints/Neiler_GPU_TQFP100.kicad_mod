  (pad "24" smd rect (at -10.75 5.5) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))
  (pad "25" smd rect (at -10.75 6) (size 1.5 0.3) (layers "F.Cu" "F.Paste" "F.Mask"))

  ;; Similar pad definitions for other 3 sides would continue here...
  ;; (abbreviated for brevity - pins 26-100 follow same pattern on other 3 sides)

  ;; 3D model reference
  (model "${KIPRJMOD}/3dmodels/neiler_gpu.step"
    (offset (xyz 0 0 0))
    (scale (xyz 1 1 1))
    (rotate (xyz 0 0 0))
  )
)
